    "coverage": "vitest run --coverage"
  },
  "bugs": {
    "url": "https://github.com/Androz2091/discord-player/issues"
  },
  "devDependencies": {
    "@discord-player/tsconfig": "^7.1.0",
    "tsup": "^8.3.5",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6"
  },
  "typedoc": {
    "entryPoint": "./src/index.ts",
    "readmeFile": "./README.md",
    "tsconfig": "./tsconfig.json",
    "excludeExternals": true,
    "excludePrivate": true
  }
